{
  "name": "kpop-recommender",
  "displayName": "kpop-recommender",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "custom-activitybar",
          "title": "In Your Face",
          "icon": "assets/logo_bito.svg"
        }
      ]
    },
    "views": {
      "custom-activitybar": [
        {
          "type": "webview",
          "id": "in-your-face.openview",
          "name": "In Your Face",
          "contextualTitle": "In Your Face"
        }
      ]
    },
    "configuration": {
      "title": "InYourFace",
      "properties": {
        "InYourFace.error.usewarnings": {
          "type": "boolean",
          "default": false,
          "description": "Should take in account warnings"
        }
      }
    },
    "commands": [
      {
        "command": "in-your-face.openview.focus",
        "category": "In Your Face",
        "title": "Open Sidebar",
        "icon": "$(clear-all)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "watch": {
    "files": "**/*.ts"
  },
  "devDependencies": {
    "@types/vscode": "^1.79.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
